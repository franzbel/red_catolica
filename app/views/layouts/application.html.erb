<!DOCTYPE html>
<html>
<head>
  <title>Redcatolica</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <% if logged_in? %>
      <h>Estas con sesion</h>
      <%= current_user.name %>
      <%= link_to "Log out", sign_out_path, method: :delete %>
  <% else %>
      <k>Estas sin sesion</k>
  <% end %>

  <%= yield %>
</div>


</body>
</html>
